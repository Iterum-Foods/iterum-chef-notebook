    "servings": 1,
    "prepTime": 15,
    "cookTime": 0,
    "totalTime": 15,
    "difficulty": "Medium",
    "components": [
      {
        "name": "Spicy Citrus Broth",
        "yield": "1 qt (800g)",
        "shelfLife": "3 days",
        "dailyPar": "1 qt",
        "portionSize": "70g",
        "ingredients": [
          {

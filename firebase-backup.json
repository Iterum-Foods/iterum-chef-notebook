{
  "firestore": {
    "rules": "firestore.rules"
  },
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**",
      "usta-public/**",
      "pages/**",
      "test-pages/**",
      "docs-markdown/**",
      "data-files/**",
      "config-files/**",
      "scripts/**",
      "*.md",
      "*.bat"
    ],
    "cleanUrls": true,
    "trailingSlash": false
  }
}
